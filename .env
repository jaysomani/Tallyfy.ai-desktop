# Application Environment Variables

# Environment (development or production)
NODE_ENV=development

# AWS Cognito settings
AWS_REGION=us-east-1
AWS_USER_POOL_ID=us-east-1_xxxxxxxxxxxx
AWS_CLIENT_ID=1xxxxxxxxxxxxxxxxxxxx

# AWS Database settings (remove AWS credentials, use placeholders)
AWS_DB_HOST=your-db-host.rds.amazonaws.com
AWS_DB_PORT=5432
AWS_DB_NAME=tallyfy
AWS_DB_USER=username
AWS_DB_PASSWORD=password
AWS_DB_SSL=true

# Encryption key
ENCRYPTION_KEY=tallyfy-encryption-key-123456

# Tally settings
TALLY_URL=http://127.0.0.1:9000

# Application version
APP_VERSION=1.1.0